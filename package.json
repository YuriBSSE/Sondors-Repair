{
  "name": "sondors-repair",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prepare": "test -f ./.env || (echo 'Defaulting to a customer configuration!' && cp customer.env .env)",
    "customer": "cp customer.env .env && yarn start",
    "provider": "cp provider.env .env && yarn start",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "4.0.0",
    "@react-native-community/netinfo": "7.1.3",
    "@react-native-picker/picker": "2.2.1",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "accordion-collapse-react-native": "^1.1.0",
    "dotenv": "^16.0.0",
    "expo": "~44.0.0",
    "expo-blur": "~11.0.0",
    "expo-constants": "~13.0.2",
    "expo-document-picker": "~10.1.3",
    "expo-file-system": "~13.1.4",
    "expo-haptics": "~11.1.0",
    "expo-image-manipulator": "~10.2.0",
    "expo-image-picker": "^13.1.1",
    "expo-location": "^14.2.2",
    "expo-media-library": "~14.0.0",
    "expo-sharing": "~10.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.7",
    "firebase": "^9.6.11",
    "geofire-common": "^5.2.0",
    "jotai": "^1.6.0",
    "loadsh": "^0.0.4",
    "moment": "^2.29.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-modal-datetime-picker": "^13.1.2",
    "react-native-pager-view": "^5.4.15",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-star-rating": "^1.1.0",
    "react-native-svg": "12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-uuid": "^2.0.1",
    "react-native-web": "0.17.1",
    "stream-chat-expo": "^3.10.2",
    "tailwind-rn": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-star-rating": "^1.1.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.7",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.3.5"
  },
  "private": true
}
